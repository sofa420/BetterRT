{
  "name": "BetterRT",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "A collection of small quality of life imporovements for Roosterteeth.com",
  "homepage_url": "http://minnesofa.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://*.roosterteeth.com/*",
    "http://roosterteeth.com/*"
  ],
  "web_accessible_resources": [
    "bundle.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.roosterteeth.com/*",
        "http://roosterteeth.com/*"
      ],
      "css": [
        "inject.css"
      ]
    },
    {
      "matches": [
        "http://*.roosterteeth.com/*",
        "http://roosterteeth.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}
